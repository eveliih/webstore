{
  "name": "webstore",
  "version": "1.0.0",
  "description": "backend for webstore project",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "build:ui": "cd ./webstore/ && npm install && npm run build  && copy .\\web.config .\\dist && xcopy /I/E dist ..\\dist",
    "build:ui:ubuntu": "rm -rf dist && cd ./webstore/ && npm install && npm run build && cp -r dist ../",
    "test": "cross-env NODE_ENV=test node --test --test-concurrency=1",
    "lint:backend": "eslint .",
    "lint:frontend": "eslint ./webstore --ext .js,.jsx"
  },
  "author": "Eveliina",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "sequelize": "^6.37.3",
    "tedious": "^18.1.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.1",
    "globals": "^15.1.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0"
  }
}
